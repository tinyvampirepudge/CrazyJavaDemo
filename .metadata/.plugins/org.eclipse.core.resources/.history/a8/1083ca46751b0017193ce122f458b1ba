package lambda;

interface Eatable{
	void taste();
}
interface Flyable{
	void fly(String weather);
}
interface Addable{
	int add(int a,int b);
}

public class LambdaQs {
	
	//调用该方法需要Eatable对象
	public void eat(Eatable eatable){
		System.out.println(eatable);
		eatable.taste();
	}
	
	//调用该方法需要Flyable对象
	public void drive(Flyable f){
		System.out.println("我正在驾驶："+f);
		f.fly("【碧空如洗的晴日】");
	}

}
